#include <stdio.h>
#include <windows.h>
#include<string.h>
#include <math.h>
#include<windows.h>
void Intm(int arr[], int sz)
{
	int i = 0;
	for (i = 0; i < sz; i++)
	{
		arr[i] = 0;
	}
}
void Printf(int arr[], int sz)
{
	int i = 0;
	for (i = 0; i < sz; i++)
	{
		printf("%d ", arr[i]);
	}
}

int main()
{
	int arr[10] = { 0 };
	int sz = sizeof(arr) / sizeof(arr[0]);
	Intm(arr, sz);
	Printf(arr, sz);
	return 0;
}
